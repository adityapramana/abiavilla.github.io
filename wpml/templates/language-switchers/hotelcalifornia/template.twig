<div class="{{ css_classes }} wpml-ls-svg-flags-with-titles">
	<ul>

		{% for code, language in languages %}
			<li class="{{ language.css_classes }}">
				<a href="{{ language.url }}">
				{% if language.flag_url != '' %}
					<img src="{{ language.flag_url }}" title="{{ language.display_name }} ({{ language.native_name }})">
				{% endif %}
				{% if language.display_name != '' %}
				{{ language.code }}
				{% endif %}
				</a>
				 
			</li>
		{% endfor %}

	</ul>
</div>